[
  {
    "name": "composer",
    "image": "@@AWS_ACCOUNT_ID.dkr.ecr.eu-west-1.amazonaws.com/composer:@@VERSION",
    "essential": true,
    "memory": 1024,
    "environment": [
      {
        "name": "ENV",
        "value": "@@ENV"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0
      }
    ],
    "ulimits": [
      {
        "name": "nofile",
        "softLimit": 16384,
        "hardLimit": 16384
      }
    ]
  }
]
